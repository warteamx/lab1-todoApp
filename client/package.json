{
  "name": "lab1-todoapp-client",
  "main": "expo-router/entry",
  "version": "1.2.0",
  "description": "React Native client for Todo application built with Expo",
  "author": "warteamx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/warteamx/lab1-todoApp.git",
    "directory": "client"
  },
  "homepage": "https://github.com/warteamx/lab1-todoApp#readme",
  "keywords": [
    "react-native",
    "expo",
    "todo-app",
    "mobile",
    "typescript"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "// Development Scripts": "",
    "start": "expo start",
    "dev": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "reset-project": "node ./scripts/reset-project.js",
    "// Build Scripts": "",
    "build": "expo export --platform web --output-dir web/dist",
    "build:web": "expo export --platform web --output-dir web/dist",
    "build:version": "node scripts/build-version.js",
    "// Testing Scripts": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage",
    "// Linting & Formatting Scripts": "",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "format": "prettier --write **/*.{js,jsx,ts,tsx,json}"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.50.1",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@tanstack/react-query": "^5.83.0",
    "aes-js": "^3.1.2",
    "expo": "~53.0.9",
    "expo-auth-session": "~6.2.0",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.6",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.8",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@semantic-release/exec": "^7.1.0",
    "@testing-library/react-native": "^13.2.1",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "cross-env": "^10.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.9",
    "prettier": "^3.6.2",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "testMatch": [
      "**/__tests__/**/*.test.{js,jsx,ts,tsx}",
      "**/?(*.)+(spec|test).{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "\\.skip\\.",
      "\\.utils\\.",
      "\\.helper\\."
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/*.d.ts",
      "!**/expo-env.d.ts",
      "!**/*.config.js",
      "!**/scripts/**",
      "!**/web/**",
      "!**/assets/**",
      "!**/__tests__/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json"
    ]
  }
}
